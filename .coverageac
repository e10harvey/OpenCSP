[run]
omit = test_*.py
       __init__.py
branch = True
dynamic_context = test_function

[report]
skip_covered = False
fail_under = 99
show_missing = True

[html]
directory = tests/htmlcov
title = GenConfig Coverage Report
show_contexts = True

[xml]
output = tests/coverage.xml

[json]
output = tests/coverage.json
pretty_print = True
show_contexts = True
